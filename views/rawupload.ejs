<html>
<%- include('header', {title:'A Luan'}) -%>
<script src="/javascripts/fileinput.min.js"></script>
<script src="/javascripts/fileinput_locale_zh.js"></script>
<link href="/stylesheets/fileinput.min.css" media="all" rel="stylesheet" type="text/css" />
  <body>

  <div id="container" class="container">
    <form class="well" role="form" method="post" enctype='multipart/form-data'>
    <h2 class="form-signin-heading">上传原图</h2>
        <div class="row">
    <input id="fulAvatar" name="fulAvatar" type="file" class="file" data-show-upload="false" data-preview-file-type="image" />
    </div>
    <br/>
    <label for="name">选择列表</label>
         <select name="position" class="form-control">
         <option value='nose'>鼻子</option>
         <option value='eyes'>眼睛</option>
         <option value='mouth'>嘴</option>
         <option value='other'>其他</option>
      </select>
<button id="btnSub" class="btn btn-lg btn-primary" type="submit">upload</button>          
</form>  
  </div>
  </body>
</html>

